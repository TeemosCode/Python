    " \n",
    "conn = sqlite3.connect(\"mydatabase.db\")\n",
    "cursor = conn.cursor()\n",
    " \n",
    "sql = \"\"\"\n",
    "DELETE FROM albums\n",
    "WHERE artist = 'John Doe'\n",
    "\"\"\"\n",
    "cursor.execute(sql)\n",
    "conn.commit()"
